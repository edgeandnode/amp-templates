{
  "name": "amp-templ-backend-apollo-graphql-gateway",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:docker": "tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/fastify": "^3.1.0",
    "@edgeandnode/amp": "^0.0.33",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.2",
    "@graphql-codegen/typescript": "^5.0.3",
    "@graphql-codegen/typescript-resolvers": "^5.1.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
