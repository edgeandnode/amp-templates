{
  "extends": "../../../tsconfig.base.jsonc",
  "compilerOptions": {
    "composite": true,
    "outDir": "dist",
    "rootDir": "src",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "allowImportingTsExtensions": false,
    "lib": [
      "ES2022"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}