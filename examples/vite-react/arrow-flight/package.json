{
  "name": "amp-templ-portfolio-arrow-flight-atom",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:proxy\" \"pnpm dev:vite\"",
    "dev:proxy": "amp proxy --port 3001 --flight-url http://localhost:1602",
    "dev:vite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "prettier": "prettier . --write --list-different",
    "prettier:check": "prettier . --check",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run typecheck; pnpm run lint; pnpm run prettier",
    "preview": "vite preview"
  },
  "dependencies": {
    "@connectrpc/connect-web": "^2.1.0",
    "@edgeandnode/amp": "^0.0.33",
    "@effect-atom/atom-react": "^0.4.0",
    "@fastify/cors": "^11.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-table": "^8.21.3",
    "apache-arrow": "^21.1.0",
    "cors": "^2.8.5",
    "effect": "^3.19.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.17",
    "viem": "^2.39.0",
    "wagmi": "^2.19.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@standard-schema/spec": "^1.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.2.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
