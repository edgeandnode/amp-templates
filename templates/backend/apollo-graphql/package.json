{
  "name": "amp-apollo-graphql-flight-backend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:docker": "tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/fastify": "^3.1.0",
    "@connectrpc/connect-web": "^2.1.0",
    "@edgeandnode/amp": "^0.0.33",
    "apache-arrow": "^21.1.0",
    "dotenv": "^17.2.3",
    "effect": "^3.19.3",
    "fastify": "^5.6.2",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.2",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.19.0"
  }
}

